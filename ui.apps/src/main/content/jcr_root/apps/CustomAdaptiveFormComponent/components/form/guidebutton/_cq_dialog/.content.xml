<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title=""
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <accordion
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/accordion">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <imageUpload1
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/authoring/components/granite/form/imageupload"
                                    assetButtonText="Browse Assets"
                                    async="{Boolean}true"
                                    autoStart="{Boolean}false"
                                    class="cq-droptarget"
                                    fieldLabel="Image"
                                    fileNameParameter="./fileName"
                                    fileReferenceParameter="./fileReference"
                                    mimeTypes="[image]"
                                    multiple="{Boolean}false"
                                    name="./file"
                                    predicate="nosystem"
                                    rootPath="/content/dam"
                                    sizeLimit="{Long}100000000"
                                    text="Upload"
                                    title="Upload Image"
                                    uploadUrl="${requestPathInfo.suffix}"
                                    useHTML5="{Boolean}true"/>

                            <fieldLayout
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <hideTitle
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <required
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <mandatoryMessage
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <placeholder
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <guideGlobalLazyField
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <validateExpMessage
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <type
                                jcr:primaryType="nt:unstructured"
                                sling:orderBefore="css"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Button Type"
                                name="./type"
                                required="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <v6
                                        jcr:primaryType="nt:unstructured"
                                        selected="{Boolean}true"
                                        text="Button"
                                        value="Button"/>
                                    <v1
                                        jcr:primaryType="nt:unstructured"
                                        text="Submit"
                                        value="submit"/>
                                    <v2
                                        jcr:primaryType="nt:unstructured"
                                        text="Reset"
                                        value="reset"/>
                                    <v3
                                        jcr:primaryType="nt:unstructured"
                                        text="Save"
                                        value="save"/>
                                    <v4
                                        jcr:primaryType="nt:unstructured"
                                        text="Previous"
                                        value="movePrev"/>
                                    <v5
                                        jcr:primaryType="nt:unstructured"
                                        text="Next"
                                        value="moveNext"/>
                                </items>
                            </type>
                            <dor jcr:primaryType="nt:unstructured">
                                <items jcr:primaryType="nt:unstructured">
                                    <excludeFromDor
                                        jcr:primaryType="nt:unstructured"
                                        checked="{Boolean}true"/>
                                </items>
                            </dor>
                            <description1
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Title"
                                    name="./description1"/>
                            <description7
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                    fieldLabel="Sub Title"
                                    name="./description7"/>
                            <alt1
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Alternate Text"
                                    name="./altText"/>

                            <checkbox1
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                    fieldLabel="Sub Title"
                                    name="./checkbox1"/>


                        </items>
                    </basic>
                    <help
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Help Content"
                        sling:resourceType="granite/ui/components/coral/foundation/container"/>
                    <patterns
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                </items>
            </accordion>
        </items>
    </content>
</jcr:root>
